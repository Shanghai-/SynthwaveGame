        <arguments>
          <argument>
            <kind>1</kind>
            <string>var shift_down = keyboard_check(vk_shift);
var right_down = keyboard_check(vk_right);
var left_down = keyboard_check(vk_left);
var space_down = keyboard_check(vk_space);
var a_down = keyboard_check(ord('A'));

if (!alive) {
    image_alpha -= 0.05;
    exit;
}

if (global.time_stopped) {
    if (alarm[0] &gt; 0) alarm[0]++;
    if (alarm[2] &gt; 0) alarm[2]++;
    if (alarm[4] &gt; 0) alarm[4]++;
    if (alarm[5] &gt; 0) alarm[5]++;
    exit;
}

if (state == player_state.solo_time) {
    if (space_down &amp;&amp; can_attack) {
        image_index = 3;
        var hitspace = global.spot_array[spot_index + (range * attack_direction)];
        hurtbox = instance_create(hitspace.x, hitspace.y, objHurtboxPlayer);
        hurtbox.blockable = false;
        can_attack = false;
        alarm[1] = global.quarter_note;
    }
    exit;
}

if (!moving_left &amp;&amp; !moving_right) {
    next_spot_index = spot_index;
    if (wait_for_beat) {
        // do nothing
    } else if (a_down) {
        image_index = 1;
        state = player_state.countering;
        alarm[1] = 8;
        //alarm[1] = 12;
        alarm[5] = global.quarter_note * 2;
        wait_for_beat = true;
    } else if (space_down) {
        image_index = 4;
        alarm[0] = global.quarter_note;
        alarm[5] = global.quarter_note;
        var hitspace = global.spot_array[spot_index + (range * attack_direction)];
        hurtbox = instance_create(hitspace.x, hitspace.y, objHurtboxPlayer);
        hurtbox.blockable = true;
        wait_for_beat = true;
    } else if (right_down) {
        next_spot_index = spot_index;
        if (shift_down &amp;&amp; spot_index &lt; global.num_spots - 2 
        &amp;&amp; global.spot_array[spot_index + 2].occupied == false) {
            moving_right = true;
            dodging = true;
            next_spot_index = spot_index + 2;
            hspeed = 60; 
        } else if (spot_index &lt; global.num_spots - 1) {
            if (global.spot_array[spot_index + 1].occupied == false) {
                moving_right = true;
                next_spot_index = spot_index + 1;
                hspeed = 20; 
            }
        }
    } else if (left_down) {
        next_spot_index = spot_index;
        if (shift_down &amp;&amp; spot_index &gt; 1 
        &amp;&amp; global.spot_array[spot_index - 2].occupied == false) {
            moving_left = true;
            dodging = true;
            next_spot_index = spot_index - 2;
            hspeed = -60;
        } else if (spot_index &gt; 0) {
            if (global.spot_array[spot_index - 1].occupied == false) {
                moving_left = true;
                next_spot_index = spot_index - 1;
                hspeed = -20;
            }
        }
    }
    if (moving_left || moving_right) {
        next_spot = global.spot_array[next_spot_index];
        next_spot.occupied = true;
        global.spot_array[spot_index].occupied = false;
    }
} else {
    if (moving_right &amp;&amp; x &gt;= next_spot.x) {
        moving_right = false;
        hspeed = 0;
        dodging = false;
        spot_index = next_spot_index;
        x = next_spot.x;
    } else if (moving_left &amp;&amp; x &lt;= next_spot.x) {
        moving_left = false;
        hspeed = 0;
        dodging = false;
        spot_index = next_spot_index;
        x = next_spot.x;
    }
}

if (dodging) {
    part_particles_create(global.particle_system, x, y, blur_particle, 1);
}

if (instance_find(objEnemy, 0).x &lt; x) {
    image_xscale = -1;
    attack_direction = -1;
} else {
    image_xscale = 1;
    attack_direction = 1;
}

</string>
